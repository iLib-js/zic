{
    "name": "zic",
    "version": "1.0.0",
    "main": "./index.js",
    "bin": "./index.js",
    "description": "A zone info compiler for IANA tzdata files",
    "license": "Apache-2.0",
    "keywords": [
        "internationalization",
        "i18n",
        "globalization",
        "g11n",
        "timezone",
        "zone"
    ],
    "email": "ehoogerbeets@gmail.com",
    "author": {
        "name": "Edwin Hoogerbeets",
        "email": "ehoogerbeets@gmail.com"
    },
    "contributors": [
        {
            "name": "Edwin Hoogerbeets",
            "email": "ehoogerbeets@gmail.com"
        }
    ],
    "files": [
        "src",
        "lib",
        "log4js.json",
        "zic.js",
        "README.md",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:iLib-js/zic.git"
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "build": "npm run build:dev",
        "build:prod": "grunt babel --mode=prod",
        "build:dev": "grunt babel --mode=dev",
        "dist": "npm run build:prod ; npm pack",
        "test": "node test/testSuite.js",
        "debug": "npm run build:dev ; node --inspect-brk test/testSuite.js",
        "clean": "git clean -f -d * ; rm -rf lib"
    },
    "dependencies": {
        "ilib-es6": "^14.4.0",
        "log4js": "^6.1.2",
        "pretty-data": "^0.40.0",
        "promise": "^8.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/polyfill": "^7.8.7",
        "@babel/preset-env": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "assertextras": "^1.0.1",
        "grunt": "^1.1.0",
        "grunt-babel": "^8.0.0",
        "grunt-cli": "^1.3.2",
        "grunt-contrib-uglify": "^4.0.1",
        "load-grunt-tasks": "^5.1.0",
        "nodeunit": "^0.11.3"
    }
}
